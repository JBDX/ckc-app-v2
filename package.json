{
  "name": "ckc-server",
  "version": "1.0.0",
  "description": "Server for the Cookies Challenge App",
  "main": "server/server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "db:migrate": "knex migrate:latest --knexfile server/knexfile.js",
    "db:seed": "knex seed:run --knexfile server/knexfile.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "pg-connection-string": "^2.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}